{% extends 'base.html.twig' %}

{% block title %}Nous Contacter{% endblock %}

{% block body %}

    {{ form_start(form) }}
    <div class="container">

    <div class="form-group">{{ form_row(form.name, {'attr':{'class':'form-control'}}) }}</div>
    <div class="form-group">{{ form_row(form.email, {'attr':{'class':'form-control'}}) }}</div>
    <div class="form-group">{{ form_row(form.messageContact, {'attr':{'class':'form-control'}}) }}</div>
    

    {{ form_end(form) }}
    </div>

{% endblock %}
